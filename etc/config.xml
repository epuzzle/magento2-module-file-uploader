<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <epuzzle_file_uploader>
            <settings>
                <allowed_extensions>jpg,jpeg,pdf,doc,png,zip</allowed_extensions>
                <media_directory>epuzzle_files</media_directory>
